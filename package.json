{
  "name": "project-template",
  "version": "0.0.1",
  "description": "A javascript project template",
  "author": "@geostarters",
  "main": "index.html",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/geostarters/react-project-template.git"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^15.2.0",
    "coveralls": "^3.0.0",
    "css-modulesify": "^0.28.0",
    "derequire": "^2.0.6",
    "documentation": "^5.3.5",
    "envify": "^4.1.0",
    "eslint": "^4.16.0",
    "eslint-config-mourner": "^2.0.3",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.6.1",
    "execcommand-copy": "^1.1.0",
    "flow-bin": "^0.64.0",
    "flow-coverage-report": "^0.4.1",
    "flow-remove-types": "^1.2.3",
    "github-slugger": "^1.2.0",
    "glob": "^7.1.2",
    "in-publish": "^2.0.0",
    "minifyify": "^7.3.5",
    "npm-run-all": "^4.1.2",
    "package-json-versionify": "^1.0.4",
    "present": "^1.0.0",
    "prismjs": "^1.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "st": "^1.2.2",
    "watchify": "^3.10.0"
  },
  "browserify": {
    "transform": [
      "unflowify",
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react",
            "stage-0"
          ],
          "plugins": [
            [
              "transform-runtime",
              {
                "polyfill": false,
                "regenerator": true
              }
            ]
          ]
        }
      ],
      [
        "bubleify",
        {
          "bubleError": true,
          "transforms": {
            "dangerousForOf": true
          },
          "objectAssign": "Object.assign"
        }
      ],
      "package-json-versionify",
      "unassertify",
      "brfs",
      "envify"
    ]
  },
  "scripts": {
    "build-dev": "browserify -p [ css-modulesify -o ./dist/index.css ] src/index.js -o dist/index.dev.js",
    "watch-dev": "watchify src/index.js -p [ css-modulesify -o ./dist/index.css ] --debug --plugin [minifyify --map index.dev.js.map --output dist/index.dev.js.map] --ignore-transform unassertify --outfile dist/index.dev.js --verbose",
    "build-min": "browserify src/index.js -p [ css-modulesify -o ./dist/index.css ] --debug --plugin [minifyify --map index.js.map --output dist/index.js.map]  | derequire > dist/index.js",
    "start-server": "st --no-cache -H localhost --port 9966 --index index.html .",
    "start": "run-p watch-dev start-server",
    "build-docs": "documentation build --github --format json --config ./docs/documentation.yml --output docs/components/api.json src/index.js",
    "lint": "eslint --fix --cache --ignore-path .gitignore src test bench docs",
    "lint-docs": "documentation lint src/index.js",
    "test": "run-s lint test-unit",
    "test-unit": "tap --reporter classic --no-coverage test/unit",
    "test-flow": "flow .",
    "test-cov": "nyc --require=flow-remove-types/register --reporter=text-summary --reporter=lcov --cache npm run test-unit",
    "pre-production": "run-s lint build-min test test-cov"
  },
  "files": [
    "build/",
    "dist/",
    "flow-typed/",
    "src/",
    ".flowconfig"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "brfs": "^1.4.4",
    "bubleify": "^1.1.0",
    "mapbox-gl": "^0.46.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-query-params": "^1.0.7",
    "react-router-dom": "^4.2.2",
    "semantic-ui-react": "^0.80.2",
    "tap": "^11.1.0",
    "unassertify": "^2.1.0",
    "unflowify": "^1.0.1"
  }
}
